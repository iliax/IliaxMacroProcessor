MACRO M [ x ]
  aadd qw 12

  before while

  SET size 5  

  WHILE [ x > size ]
    DEC x
    print => x

    IF [ x == size ]
	'x' == size - break
    ENDIF

  ENDW

  MACRO mac []
    ololo
  MEND

  after while
  GOTO E1
  // skip this..
  E1: ending

MEND


